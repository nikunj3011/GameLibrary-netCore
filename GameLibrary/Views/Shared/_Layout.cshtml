<html>
<head>
    <meta charset="utf-8" />
    <title>Game Library</title>
    <link rel="stylesheet" href="css/style.css">
</head>
<body>
    <h1>
        Welcome to game Library: @ViewBag.Title
    </h1>
    <h2>@ViewBag.Title</h2>
    <menu>
        <ul>
            <li><a asp-controller="App" asp-action="Index">Home</a></li>
            <li><a asp-controller="App" asp-action="Contact">Contact</a></li>
            <li><a asp-controller="App" asp-action="AboutUs">About</a></li>
        </ul>
    </menu>
    @RenderBody()
    <script src="~/node-modlues/jquery/dist/jquery.min.js"></script>
    @RenderSection("scripts",false)
</body>
</html>